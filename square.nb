Cell[BoxData[
 RowBox[{
  RowBox[{"(*", "periodicity", "*)"}], "\n", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"siteIndex", "[", 
      RowBox[{"m_", ",", "n_"}], "]"}], ":=", 
     RowBox[{
      RowBox[{"Mod", "[", 
       RowBox[{"m", ",", "Lx"}], "]"}], "+", 
      RowBox[{"Lx", "*", 
       RowBox[{"Mod", "[", 
        RowBox[{"n", ",", "Ly"}], "]"}]}]}]}], ";"}], "\n", 
   "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
    "Generates", " ", "all", " ", "horizontal", " ", "or", " ", "vertical", 
     " ", "bonds"}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"generateBonds", "[", 
      RowBox[{"dir_", ",", "periodic_"}], "]"}], ":=", 
     RowBox[{"Module", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"rangeM", ",", "rangeN", ",", "pairFn"}], "}"}], ",", 
       RowBox[{
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"dir", "===", "\"\<horizontal\>\""}], ",", 
          RowBox[{
           RowBox[{"rangeM", "=", 
            RowBox[{"If", "[", 
             RowBox[{"periodic", ",", 
              RowBox[{"Range", "[", 
               RowBox[{"0", ",", 
                RowBox[{"Lx", "-", "1"}]}], "]"}], ",", 
              RowBox[{"Range", "[", 
               RowBox[{"0", ",", 
                RowBox[{"Lx", "-", "2"}]}], "]"}]}], "]"}]}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"rangeN", "=", 
            RowBox[{"Range", "[", 
             RowBox[{"0", ",", 
              RowBox[{"Ly", "-", "1"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
           RowBox[{"pairFn", "=", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{
               RowBox[{"siteIndex", "[", 
                RowBox[{"#", ",", "#2"}], "]"}], ",", 
               RowBox[{"siteIndex", "[", 
                RowBox[{
                 RowBox[{"#", "+", "1"}], ",", "#2"}], "]"}]}], "}"}], 
             "&"}]}], ";"}], ",", 
          RowBox[{
           RowBox[{"rangeM", "=", 
            RowBox[{"Range", "[", 
             RowBox[{"0", ",", 
              RowBox[{"Lx", "-", "1"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
           RowBox[{"rangeN", "=", 
            RowBox[{"If", "[", 
             RowBox[{"periodic", ",", 
              RowBox[{"Range", "[", 
               RowBox[{"0", ",", 
                RowBox[{"Ly", "-", "1"}]}], "]"}], ",", 
              RowBox[{"Range", "[", 
               RowBox[{"0", ",", 
                RowBox[{"Ly", "-", "2"}]}], "]"}]}], "]"}]}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"pairFn", "=", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{
               RowBox[{"siteIndex", "[", 
                RowBox[{"#", ",", "#2"}], "]"}], ",", 
               RowBox[{"siteIndex", "[", 
                RowBox[{"#", ",", 
                 RowBox[{"#2", "+", "1"}]}], "]"}]}], "}"}], "&"}]}], ";"}]}],
          "]"}], ";", "\[IndentingNewLine]", 
        RowBox[{"Sort", "/@", 
         RowBox[{"Flatten", "[", 
          RowBox[{
           RowBox[{"Table", "[", 
            RowBox[{
             RowBox[{"pairFn", "[", 
              RowBox[{"m", ",", "n"}], "]"}], ",", 
             RowBox[{"{", 
              RowBox[{"n", ",", "rangeN"}], "}"}], ",", 
             RowBox[{"{", 
              RowBox[{"m", ",", "rangeM"}], "}"}]}], "]"}], ",", "1"}], 
          "]"}]}]}]}], "]"}]}], ";"}], "\n", "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{"Bonds", " ", "for", " ", "each", " ", "case"}], "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"bondsPBCm", "=", 
     RowBox[{"Join", "[", 
      RowBox[{
       RowBox[{"generateBonds", "[", 
        RowBox[{"\"\<horizontal\>\"", ",", "True"}], "]"}], ",", 
       RowBox[{"generateBonds", "[", 
        RowBox[{"\"\<vertical\>\"", ",", "False"}], "]"}]}], "]"}]}], ";"}], 
   "\n", 
   RowBox[{
    RowBox[{"bondsPBCn", "=", 
     RowBox[{"Join", "[", 
